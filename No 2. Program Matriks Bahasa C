#include <stdio.h>
#include <stdlib.h>

int A[3][3],i,j;

int main(int argc, char *argv[])
{
 for(i=0;j=i<3;i++)
 {
  for(j=0;j<3;j++)
  {
   printf("Input Matriks A[%d][%d]: ",i+1,j+1);
   scanf("%d",&A[i][j]);
  }
 }
 for(i=0;i<3;i++)
 {
  for(j=0;j<3;j++)
  {
   printf("%3d",A[i][j]);
  }
  printf("\n");
 }
 system("pause");
 return 0;
}
